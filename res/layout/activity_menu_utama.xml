<TableLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/TableLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >
                       
		<TableRow
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content" 
		    android:gravity="center">
        
		<RelativeLayout >
	   		<Button
              android:id="@+id/btnwisata"
              android:drawableTop="@drawable/wisata"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:height="140dp"
              android:width="140dp"
			  android:text="WISATA"
			  android:onClick="wisata"/>
		</RelativeLayout>

			<Button
			  android:id="@+id/btnmaps"
			  android:drawableTop="@drawable/maps"
			  android:layout_width="wrap_content"
			  android:layout_height="wrap_content"
			  android:gravity="center"
			  android:height="140dp"
			  android:width="140dp"
			  android:text="MAPS"
			  android:onClick=""/>
		</TableRow>

		<TableRow 
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content" 
		    android:gravity="center">
			
		<RelativeLayout >
            <Button
	          android:id="@+id/btntmg"
	          android:drawableTop="@drawable/tmg"
	          android:layout_width="wrap_content"
	          android:layout_height="wrap_content"
	          android:height="140dp"
	          android:width="140dp"
	          android:text="TEMANGGUNG"
	          android:textAppearance="?android:attr/textAppearanceSmall"
	          android:onClick=""/>
		</RelativeLayout>

			<Button
	 		  android:id="@+id/btncari"
		      android:drawableTop="@drawable/cari"
		      android:layout_width="wrap_content"
		      android:layout_height="wrap_content"
		      android:height="140dp"
		      android:width="140dp"
		      android:text="CARI"
		      android:onClick=""/>
		</TableRow>

		<TableRow 
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content" 
		    android:gravity="center">
			
		<RelativeLayout >
            <Button
              android:id="@+id/btnkomentar"
              android:drawableTop="@drawable/komentar"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content" 
              android:height="140dp"
              android:width="140dp"
              android:text="KOMENTAR"
              android:onClick=""/>
		</RelativeLayout>

			<Button
			  android:id="@+id/btnoption"
			  android:drawableTop="@drawable/option"
		      android:layout_width="wrap_content"
	          android:layout_height="wrap_content"
		      android:height="140dp"
		      android:width="140dp"
		      android:text="OPTION"
		      android:onClick=""/>
		</TableRow>

</TableLayout>